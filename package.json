{
  "name": "metalsmith-shopify",
  "description": "Why push every change to a server-rendered theme? Cache Shopify data and build themes with Metalsmith.",
  "main": "dist/index.js",
  "scripts": {
    "test": "mocha --compilers js:babel-register",
    "test:watch": "mocha --compilers js:babel-register -w",
    "build": "rimraf ./dist && babel src -d dist",
    "check-coverage": "./node_modules/.bin/babel-node ./node_modules/.bin/istanbul check-coverage --statements 100 --branches 100 --functions 100 --lines 100",
    "cover": "./node_modules/.bin/babel-node ./node_modules/.bin/istanbul cover -x *.spec.js ./node_modules/.bin/_mocha -- test/index.spec.js -R spec --recursive --compilers js:babel-register",
    "report-coverage": "./node_modules/.bin/codecov -t a5a0a03e-6504-45d1-8eba-54ee0c290fce",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/cameronroe/metalsmith-shopify.git"
  },
  "babel": {
    "presets": [
      "es2015"
    ]
  },
  "files": [
    "dist",
    "README.md"
  ],
  "author": "Cameron Roe <cameronjroe@gmail.com> (http://cameronroe.com)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/cameronroe/metalsmith-shopify/issues"
  },
  "homepage": "https://github.com/cameronroe/metalsmith-shopify#readme",
  "dependencies": {
    "axios": "0.11.0",
    "codecov": "1.0.1",
    "env2": "2.0.7",
    "lodash": "4.11.2",
    "metalsmith": "2.1.0",
    "q": "1.4.1",
    "shopify-api-node": "1.0.0"
  },
  "devDependencies": {
    "babel-cli": "6.8.0",
    "babel-preset-es2015": "6.6.0",
    "babel-preset-stage-0": "6.5.0",
    "babel-register": "6.8.0",
    "chai": "3.5.0",
    "istanbul": "1.0.0-alpha.2",
    "metalsmith-layouts": "1.6.5",
    "mocha": "2.4.5",
    "nock": "8.0.0",
    "rewire": "2.5.1",
    "rimraf": "2.5.2",
    "semantic-release": "^4.3.5",
    "sinon": "1.17.4",
    "tinyliquid": "0.2.31"
  }
}
